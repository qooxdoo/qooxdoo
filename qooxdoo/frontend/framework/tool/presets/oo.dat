###########################################################################
#
# Generates a compiled qooxdoo core JavaScript file
#
###########################################################################

class-path = framework/source/class
cache-directory = framework/.cache
generate-compiled-script
compiled-script-file = oo.js
optimize-strings
optimize-variables
optimize-private
add-file-ids
use-variant = qx.debug:off

include = qx.Clazz
