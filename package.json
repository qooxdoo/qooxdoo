  "version": "0.4.1",
  "keywords": [
    "qooxdoo",
    "compiler",
    "cli"
  ],
  "homepage": "https://github.com/qooxdoo/qooxdoo-compiler",
  "repository": {
    "type": "git",
    "url": "https://github.com/qooxdoo/qooxdoo-compiler.git"
  },
  "bugs": {
    "url": "https://github.com/qooxdoo/qooxdoo-compiler/issues"
  },
  "license": "MIT",
