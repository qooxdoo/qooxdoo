    "common-XXX" :
    {
      "library" :
      [
        {
          "manifest" : "../../framework/Manifest.json",
          "uri"      : "../../../../../framework"
        },

        {
          "manifest" : "Manifest_samples.json",
          "uri"      : "../../.."
        }
      ],

      "include" :
      [
        "XXX",
        "qx.log.appender.Native",
        "qx.log.appender.Console"        
      ],

      "require" :
      {
      	"qx.ui.core.Widget" : ["qx.theme.Classic"]
      },

      "themes" :
      {
        "qx.icontheme" : ["Oxygen"],
        "qx.theme"     : ["Modern"]
      },

      "cache" :
      {
        "path" : "../../cache"
      },

      "localize" :
      {
        "locales" : ["en", "en_US", "de", "de_DE"]
      }
    },

    "source-XXX" :
    {
      "extend" : ["common-XXX"],

      "settings" :
      {
        "qx.application" : "XXX"
      },

      "compile-source" :
      {
        "file" : "source/script/XXX.js"
      }
    },

    "build-XXX" :
    {
      "extend" : ["common-XXX"],

      "settings" :
      {
        "qx.application" : "XXX"
      },

      "variants" :
      {
        "qx.debug" : ["off"]
      },

      "compile-dist" :
      {
        "file" : "build/script/XXX.js",
        "uri"  : "../../script/XXX.js",
        "resourceUri" : "../..",
        "optimize" : [ "variables", "basecalls", "privates" ],
        "format" : "on"
      }
    }
